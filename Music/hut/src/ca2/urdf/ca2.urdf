    <rightRearJoint>wheel1j</rightRearJoint>
    <wheelSeparation>0.613</wheelSeparation>
    <wheelDiameter>0.280</wheelDiameter>
    <robotBaseFrame>base_link</robotBaseFrame>
    <odometryTopic>odom</odometryTopic>
